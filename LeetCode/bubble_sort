#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n,temp;
    cout<<"enter the numer of elements"<<endl;
    cin>>n;
    int a[n];
    cout<<"enter "<<n<<" elements"<<endl;
    for(int i=0;i<n;i++)cin>>a[i];
    for(int i=0;i<n;i++){
        for(int j=0;j<n-i-1;j++)
        {
            if(a[j]>a[j+1])
            {
                temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;
            }
        }
    }
    cout<<"the sorted array is: ";
    for(int i=0;i<n;i++)cout<<a[i]<<" ";
    return 0;
}
